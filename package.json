{
  "name": "RCNode",
  "version": "6.0.0",
  "private": true,
  "author": "Fansaly",
  "description": "Node.js Chinese professional community CNode's third-party WEB APP.",
  "keywords": [
    "react",
    "react-component",
    "material design",
    "material-ui",
    "cnodejs",
    "cnode"
  ],
  "license": "MIT",
  "homepage": "https://fansaly.github.io/RCNode",
  "repository": {
    "type": "git",
    "url": "https://github.com/fansaly/RCNode.git"
  },
  "bugs": {
    "url": "https://github.com/fansaly/RCNode/issues"
  },
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "check": "pnpm check:lint && pnpm check:prettier && pnpm check:type",
    "check:type": "tsc",
    "check:lint": "eslint . --ext .jsx,.js,.ts,.tsx --ignore-path ./.eslintignore",
    "check:prettier": "prettier --loglevel warn --debug-check \"./**/*.{js,jsx,ts,tsx,json,css}\"",
    "fix": "pnpm fix:lint && pnpm fix:prettier",
    "fix:lint": "eslint . --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.eslintignore",
    "fix:prettier": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,json,css}\"",
    "deps:check": "node packages/deps-check",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@mui/styles": "^5.8.7",
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.28.0",
    "clipboard-copy": "^4.0.1",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.3",
    "github-markdown-css": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-markdown": "^8.0.3",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scroll": "^1.8.7",
    "react-swipeable-views": "^0.14.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-transition-group": "^4.4.2",
    "redux": "^4.2.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-raw": "^6.1.1",
    "rehype-video": "^1.2.2",
    "remark-gemoji": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-scroll": "^1.8.3",
    "@types/react-swipeable-views": "^0.13.1",
    "@types/react-syntax-highlighter": "^15.5.2",
    "@types/react-transition-group": "^4.4.5",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "redux-logger": "^3.0.6",
    "stylus": "^0.58.1",
    "typescript": "^4.7.4",
    "vite": "2.9.17",
    "vite-plugin-checker": "^0.4.8",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-svgr": "^2.2.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=6.0.0"
  }
}
